{
  "name": "airbitz-core-js-ui",
  "version": "0.1.5",
  "description": "Airbitz login UI components, as a library",
  "keywords": [
    "Airbitz",
    "bitcoin",
    "blockchain",
    "ethereum",
    "key management",
    "security"
  ],
  "license": "SEE LICENSE IN LICENSE",
  "author": "Airbitz, Inc.",
  "files": [
    "assets/**",
    "lib/**",
    "src/common/**",
    "src/native/**",
    "ios/**",
    "android/**"
  ],
  "main": "lib/abcui.cjs.js",
  "module": "lib/abcui.es6.js",
  "scripts": {
    "build": "rimraf assets lib && rollup -c && webpack -p --color --progress",
    "fix": "standard --parser babel-eslint --fix",
    "lint": "standard --parser babel-eslint",
    "precommit": "npm run lint",
    "prepare": "npm run build",
    "start": "webpack-dev-server --open"
  },
  "dependencies": {
    "airbitz-core-js": "git+ssh://git@github.com/Airbitz/airbitz-core-js.git#develop",
    "async": "~2.0.1",
    "babel-preset-stage-2": "^6.24.1",
    "classnames": "^2.2.5",
    "file-loader": "~0.9.0",
    "joi-browser": "~9.0.1",
    "jsbarcode": "~3.5.8",
    "lodash": "~4.17.4",
    "moment": "~2.14.1",
    "moment-duration-format": "^1.3.0",
    "normalize.css": "v4.2.0",
    "qrcode.react": "~0.6.1",
    "react": "~15.4.2",
    "react-addons-update": "v15.3.0",
    "react-css-themr": "v1.7.1",
    "react-dom": "^15.4.2",
    "react-native-keyboard-aware-scroll-view": "^0.4.1",
    "react-native-linear-gradient": "^2.0.0",
    "react-native-material-textfield": "git+ssh://git@github.com:ahujsak/react-native-material-textfield.git#addOnFinish",
    "react-redux": "~4.4.5",
    "react-router": "~2.6.0",
    "react-toolbox": "v1.1.2",
    "redux": "~3.5.2",
    "redux-form": "~5.3.1",
    "redux-logger": "~2.7.4",
    "redux-thunk": "~2.1.0",
    "socket.io-client": "~1.7.2",
    "sprintf-js": "^1.0.3",
    "superagent": "~2.1.0",
    "whatwg-fetch": "^2.0.3",
    "zxcvbn": "^4.4.2"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.2",
    "babel": "^6.23.0",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.1.1",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.4",
    "encoding": "^0.1.12",
    "enzyme": "^2.8.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "html-webpack-plugin": "^2.30.1",
    "husky": "^0.14.3",
    "node-sass": "^4.5.3",
    "postcss-loader": "^2.0.6",
    "react-addons-test-utils": "^15.4.2",
    "react-hot-loader": "^1.3.1",
    "react-test-renderer": "^15.4.2",
    "redux-devtools-extension": "^2.10.2",
    "redux-mock-store": "^1.2.2",
    "rimraf": "^2.6.1",
    "rollup": "^0.45.2",
    "rollup-plugin-babel": "^2.7.1",
    "sass-lint": "~1.10.2",
    "sass-loader": "^6.0.6",
    "standard": "~8.6.0",
    "style-loader": "^0.18.2",
    "webpack": "^3.4.1",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-dev-server": "^2.6.1"
  },
  "react-native": {
    "lib/abcui.es6.js": "src/native/index.js",
    "lib/abcui.cjs.js": "src/native/index.js"
  },
  "rnpm": {
    "assets": [
      "./src/assets/fonts"
    ]
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/fakeFile.js",
      "\\.(css|less|scss)$": "<rootDir>/fakeStyle.js"
    }
  }
}
