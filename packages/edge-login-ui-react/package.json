    "rimraf": "^2.6.3",
    "rollup": "^1.19.4",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.0.2",
    "rollup-plugin-flow-entry": "^0.3.1",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-postcss": "^2.0.3",
    "rollup-plugin-sass": "^1.2.2",
    "rollup-plugin-url": "^2.2.2",
    "sass": "^1.32.12"
