  ],
  "main": "lib/edge-login-ui-react.js",
  "scripts": {
    "build": "npm-run-all build:lib build:demo",
    "build:demo": "rimraf dist && parcel build --public-url ./ src/demo/index.html",
    "build:lib": "rimraf lib && rollup -c",
    "prepare": "npm-run-all build:lib",
    "start": "npm-run-all -p start:*",
    "start:demo": "parcel ./src/demo/index.html --open",
    "start:lib": "rollup -cw"
  },
  "dependencies": {
    "edge-core-js": "^0.13.5",
    "normalize.css": "^8.0.1",
    "qrcode.react": "~0.6.1",
    "react": "~15.4.2",
    "react-addons-update": "v15.3.0",
    "react-dom": "^15.4.2",
    "react-icons": "^3.7.0",
    "react-modal": "^3.9.1",
    "react-redux": "^5.0.7",
    "redux": "~3.5.2",
    "redux-logger": "~2.7.4",
    "redux-thunk": "~2.1.0",
    "sprintf-js": "^1.0.3",
    "whatwg-fetch": "^2.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-transform-runtime": "^7.5.5",
