{
  "name": "edge-login-ui-react",
  "version": "0.6.11",
  "description": "Edge login UI, as React components",
  "keywords": [
    "Airbitz",
    "Edge",
    "bitcoin",
    "blockchain",
    "ethereum",
    "key management",
    "security"
  ],
  "license": "SEE LICENSE IN LICENSE",
  "author": "Airbitz, Inc.",
  "files": [
    "LICENSE",
    "lib",
